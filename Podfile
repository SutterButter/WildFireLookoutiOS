# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
source 'https://github.com/CocoaPods/Specs.git'
target 'Wildfire Lookout' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Wildfire Lookout
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'Geofirestore'
  pod 'Firebase/Messaging'
  


  target 'Wildfire LookoutTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Wildfire LookoutUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end


